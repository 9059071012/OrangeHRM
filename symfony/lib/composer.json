{

  "name": "orangehrm/orangehrm",
  "description": "OrangeHRM open source",
  "keywords": ["orangehrm"],
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/orangehrm/symfony1"
    },
    {
      "type": "vcs",
      "url": "https://github.com/orangehrm/doctrine1"
    },
    {
      "type": "vcs",
      "url": "https://github.com/orangehrm/LightOpenID"
    }
  ],
  "config": {
    "vendor-dir": "vendor"
  },
  "require": {
    "oomphinc/composer-installers-extender": "^1.1",
    "iignatov/lightopenid": "dev-master",
    "phpseclib/phpseclib": "~2.0",
    "respect/validation": "1.1.31",
    "psr/log": "^1.0",
    "google/apiclient": "^2.0",
    "bjeavons/zxcvbn-php": "^0.3",
    "bshaffer/oauth2-server-php": "^1.11",
    "scssphp/scssphp": "^1.8",
    "friendsofsymfony1/symfony1": "dev-master",
    "friendsofsymfony1/doctrine1": "dev-master"
  },
  "require-dev": {
    "codeception/codeception": "^2.2",
    "phpunit/phpunit": "^5.7",
    "zircote/swagger-php": "^3.0"
  },
  "extra": {
    "installer-types": ["library"],
    "installer-paths": {
      "vendor/symfony/": ["friendsofsymfony1/symfony1"]
    }
  },
  "autoload": {
    "psr-4": {
      "Orangehrm\\Rest\\":"../plugins/orangehrmRESTPlugin/lib"
    }
  }
}
